{
  "$version": "1.0",
  "operators": [
    {
      "queryexpression": {
        "operator": "NotNull",
        "conditionPattern": "\"{propName}\" , ConditionOperator.{operator}"
      },
      "webapi": {
        "operator": "ne null",
        "conditionPattern": "'{propName}' {operator}"
      }
    },
    {
      "queryexpression": {
        "operator": "Equal",
        "conditionPattern": "\"{propName}\" , ConditionOperator.{operator}, {value}"
      },
      "webapi": {
        "operator": "eq",
        "conditionPattern": "'{propName}' {operator} {value}"
      }
    },
    {
      "queryexpression": {
        "operator": "NotEqual",
        "conditionPattern": "\"{propName}\" , ConditionOperator.{operator}, {value}"
      },
      "webapi": {
        "operator": "ne",
        "conditionPattern": "'{propName}' {operator} {value}"
      }
    },
    {
      "queryexpression": {
        "property": "GreaterThan",
        "conditionPattern": "\"{propName}\" , ConditionOperator.{operator}, {value}"
      },
      "webapi": {
        "property": "gt",
        "conditionPattern": "'{propName}' {operator} {value}"
      }
    },
    {
      "queryexpression": {
        "property": "NotIn",
        "conditionPattern": "\"{propName}\" , ConditionOperator.{operator}, new object[] \\{ {value} \\}",
        "valueRendering": "ToArray"
      },
      "webapi": {
        "property": "Microsoft.Dynamics.CRM.NotIn",
        "conditionPattern": "{operator}(PropertyName='{propName}',PropertyValues=[{value}])",
        "valueRendering": "ToArray"
      }
    },
    {
      "queryexpression": {
        "property": "Like",
        "conditionPattern": "\"{propName}\" , ConditionOperator.{operator}, \"{value}\""
      },
      "webapi": {
        "property": "like",
        "conditionPattern": "{propName} {operator} {value}",
      }
    }
  ]
}